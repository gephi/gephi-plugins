                <configuration>
                    <licenseName>BSD</licenseName>
                    <author>Stuart Turton</author>
                    <authorEmail></authorEmail>
                    <authorUrl></authorUrl>
                    <sourceCodeUrl>https://github.com/sturton/gephi-plugins</sourceCodeUrl>
                    <publicPackages>
